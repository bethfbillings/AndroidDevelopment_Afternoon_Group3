<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.flashquiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/flashquiz_icon"
        android:roundIcon="@mipmap/flashquiz_icon_round"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Edit_Flashcard"></activity>
        <activity android:name=".Statistics" />
        <activity android:name=".Flashcard_Back" />
        <activity android:name=".Flashcard_Front" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>